\newlength{\tmplength}
\newlength{\currentparskip}
\newcommand{\QuestionAndResponse}[2]{%
  \setlength{\currentparskip}{\parskip}
  \begin{minipage}{\linewidth}
  \textbf{#1}\par
  \smallskip
  \setlength{\fboxsep}{1ex}
  \ifstrequal{#2}{}
  {\fcolorbox{black}{OLGreen!10}{\begin{minipage}[t][2\baselineskip]{\linewidth}~\end{minipage}}}
  {\fbox{\begin{minipage}[t]{\linewidth}%
  \setlength{\parskip}{\currentparskip}%
  \itshape #2\end{minipage}}}
  \end{minipage}\vspace{1.25\bigskipamount}%
}

\RequirePackage{needspace}
\newenvironment{EmailMessage}[1]{%
  \Needspace{5\baselineskip}
  \textbf{#1}%
  \begin{mdframed}[topline=false,rightline=false,bottomline=false,linewidth=2pt,linecolor=gray!40]
}{%
  \end{mdframed}\medskip
}

